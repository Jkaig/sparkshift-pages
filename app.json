{
  "expo": {
    "name": "SparkShift",
    "slug": "sparkshift-pages",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeremykaigler.sparkshift",
      "buildNumber": "1.0.0",
      "associatedDomains": [
        "applinks:sparkshift.app",
        "applinks:www.sparkshift.app"
      ]
    },
    "android": {
      "package": "com.jeremykaigler.sparkshiftapp",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.sparkshift.app",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "scheme": "sparkshift",
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "entryPoint": "./index.js"
  }
}
